{
  "name": "mongoose-mob",
  "version": "1.0.4",
  "description": "set of utilities for managing mongoose connections and models",
  "author": {
    "name": "David Arata",
    "email": "dave.arata@dacatech.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/davearata/mongoose-mob.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "babel index.js --out-dir dist",
    "clean": "rm -rf dist",
    "cover": "isparta cover --report text --report html _mocha --include-all-sources --excludes dist/** -- --compilers js:babel-core/register --require babel-polyfill --require ./test/setup.js 'test/unit/**/*.js'",
    "lint-staged": "lint-staged",
    "lint": "eslint ./",
    "test": "mocha --reporter nyan --compilers js:babel-core/register --require babel-polyfill --require ./test/setup.js 'test/unit/**/*.js'",
    "prepublish": "npm run clean && npm run build"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "mongoose-mob",
    "mongoose",
    "connections",
    "models"
  ],
  "dependencies": {
    "async-exit-hook": "^1.1.1",
    "co": "^4.6.0",
    "debug": "^2.3.3",
    "lodash": "^4.17.2",
    "mongoose": "^4.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.11.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "isparta": "^4.0.0",
    "lint-staged": "^3.2.1",
    "mocha": "^3.2.0",
    "pre-commit": "^1.1.3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
